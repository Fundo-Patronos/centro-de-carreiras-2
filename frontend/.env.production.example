# Centro de Carreiras Frontend - Production Environment Variables
# Copy this file to .env.production and fill in the values

# Backend API URL (will be set automatically during deployment)
VITE_API_URL=https://centro-carreiras-api-xxxxx.run.app/api/v1

# Firebase Configuration
VITE_FIREBASE_API_KEY=AIzaSyXXXXXXXXXXXXXX
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456

# Mixpanel Analytics (optional)
VITE_MIXPANEL_TOKEN=your-mixpanel-token
